{
    "name": "cdn-ts",
    "version": "1.0.0",
    "description": "TypeScript CDN Server with file upload and management",
    "main": "dist/server.js",
    "scripts": {
        "dev": "dotenvx run -- tsx watch src/server.ts",
        "build": "tsc",
        "start": "dotenvx run -- node dist/server.js",
        "start:prod": "dotenvx run -- node dist/server.js"
    },
    "keywords": [
        "cdn",
        "file-server",
        "upload"
    ],
    "author": "Doughmination",
    "license": "SEE LICENCE IN LICENCE",
    "dependencies": {
        "@dotenvx/dotenvx": "^1.34.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "ejs": "^3.1.10",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "express-session": "^1.18.1",
        "helmet": "^8.0.0",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/ejs": "^3.1.5",
        "@types/express": "^5.0.0",
        "@types/express-session": "^1.18.1",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.10.5",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3"
